---
salt: {{ influx_salt }}
{% if db_password is defined %}
pgconn:
    main: postgres://{{ db_user }}:{{ db_password }}@{{ db_server }}:{{ db_port }}/{{ database }}
{% endif %}
influxdb:
    host: {{ influx_url }}
    database: queues
    ssl: true
    verify_ssl: true
    username: node
    password: {{ vault_influx_node_password }}